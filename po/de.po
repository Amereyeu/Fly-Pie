msgstr "<big>Letzte Auswahlzeit: <b>- ms</b></big>"

#. ms = milliseconds
#: assets/settings.ui:1314
msgid "<big>Best selection time: <b>- ms</b></big>"
msgstr "<big>Beste Auswahlzeit: <b>- ms</b></big>"

